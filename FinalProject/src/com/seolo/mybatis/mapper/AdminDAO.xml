<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
 PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.seolo.idao.IAdminDAO">

	<select id="login" resultType="com.seolo.dto.AdminDTO">
		SELECT AD_ID
		FROM ADMIN
		WHERE AD_ID = #{ad_Id}
		AND   AD_PW = #{ad_Pw}	
	</select>



	
	
</mapper>